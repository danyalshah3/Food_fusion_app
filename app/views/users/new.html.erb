<h1>Hello World!</h1>
<h1>Welcome to Food-Fusion!</h1>
<div class="container">
  <%= form_for @user do |f| %>
  <%= render partial: "layouts/errors", locals: {record: @user} %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>
    <%= f.label :email %><br>
    <%= f.email_field :email %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %><br>
    <%= f.label :zipcode %><br>
    <%= f.number_field :zipcode, in: 10001..99999 %><br>
    <%= f.submit "Sign Up!" %><br>
  <% end %>
</div>



   
      
